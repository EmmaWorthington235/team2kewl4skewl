<title>Register</title> <!-- The title of this page needs to be "Register" for test cases to work, do not edit this line -->

<%- include ('../partials/head') %>
<%- include ('../partials/menu') %>

<main>
    <div class="container">
      <!-- THE REST OF YOUR HTML CODE GOES HERE -->
      <h1>Register</h1>
      <form action="/register" method="POST">
          <div class="form-group my-2">
            <label for="InputUsername">Username</label>
            <input type="text" class="form-control" id="username" name="username" aria-describedby="emailHelp"
              placeholder="Enter Username">
          </div>
          <div class="form-group">
            <label for="InputPassword">Password</label>
            <input type="password" class="form-control" id="password" name="password" placeholder="Enter Password">
          </div>
          <button type="submit" class="btn btn-primary my-2">Register</button>
        </form>
        <small class="form-text text-muted">Already have an account? </small>
        <small class="nav-item me-1">
          <a class = "nav-link" href = "/login">Login</a>
        </small>
    </div>
 </main>
